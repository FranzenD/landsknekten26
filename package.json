{
   "name": "landsknekten26",
   "version": "0.1.0",
   "private": true,
   "scripts": {
      "serve": "vue-cli-service serve",
      "build": "vue-cli-service build",
      "lint": "vue-cli-service lint"
   },
   "dependencies": {
      "core-js": "^2.6.12",
      "dayjs": "^1.9.7",
      "firebase": "^7.24.0",
      "focus-visible": "^5.2.0",
      "luxon": "^1.25.0",
      "vue": "^2.6.12",
      "vue-datetime": "^1.0.0-beta.14",
      "vue-router": "^3.4.9",
      "vuex": "^3.6.0",
      "weekstart": "^1.0.1"
   },
   "devDependencies": {
      "@vue/cli-plugin-babel": "^3.0.0",
      "@vue/cli-plugin-eslint": "^3.0.0",
      "@vue/cli-service": "^5.0.8",
      "@vue/eslint-config-prettier": "^6.0.0",
      "babel-eslint": "^10.1.0",
      "eslint": "^5.16.0",
      "eslint-plugin-prettier": "^3.2.0",
      "eslint-plugin-vue": "^5.0.0",
      "prettier": "^1.19.1",
      "sass": "^1.30.0",
      "sass-loader": "^8.0.2",
      "vue-cal": "^2.24.8",
      "vue-template-compiler": "^2.6.12"
   },
   "prettier": {
      "singleQuote": true,
      "tabWidth": 3,
      "htmlWhitespaceSensitivity": "ignore",
      "printWidth": 80
   },
   "eslintConfig": {
      "root": true,
      "env": {
         "node": true,
         "es6": true
      },
      "extends": [
         "plugin:vue/recommended",
         "eslint:recommended",
         "prettier/vue",
         "plugin:prettier/recommended"
      ],
      "rules": {
         "no-debugger": "off",
         "prettier/prettier": "warn",
         "no-console": "off",
         "prefer-const": "warn",
         "no-unused-vars": "warn",
         "vue/require-default-prop": "off"
      },
      "parserOptions": {
         "parser": "babel-eslint",
         "ecmaVersion": 6
      },
      "plugins": [
         "prettier"
      ]
   },
   "postcss": {
      "plugins": {
         "autoprefixer": {}
      }
   },
   "browserslist": [
      "> 1%",
      "last 2 versions"
   ]
}
